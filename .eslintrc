{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "jest": true
  },
  "plugins": [
    "jest"
  ],
  "rules": {
    "import/extensions": 0,
    "arrow-parens": 0,
    "class-methods-use-this": 0,
    "no-mixed-operators": 0,
    "no-plusplus": 0,
    "wrap-iife": 0,
    "space-before-function-paren": 0
  }
}
